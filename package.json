{
  "name": "aclify",
  "version": "1.3.0",
  "description": "Node Access Control List (ACL)",
  "license": "MIT",
  "main": "dist/src/index.js",
  "scripts": {
    "clean": "rm -rf coverage dist tmp jest_0",
    "lint": "eslint .",
    "typecheck": "flow check",
    "pretest": "npm run lint",
    "test": "jest --coverage",
    "test-only": "jest",
    "test:watch": "jest --watch",
    "prepare": "npm run build",
    "build": "babel src/**/*.js src/*.js -d dist",
    "build:watch": "babel --watch src/**/*.js src/*.js -d dist",
    "docker:run:test": "docker-compose up"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rimiti/aclify.git"
  },
  "keywords": [
    "node",
    "redis",
    "mongodb",
    "mysql",
    "sqlite",
    "postgresql",
    "mssql",
    "memory",
    "api",
    "apikey",
    "key",
    "acl",
    "express",
    "manager"
  ],
  "author": {
    "name": "Dimitri DO BAIRRO",
    "email": "dimitri.dobairro@dimsolution.com",
    "url": "dimsolution.com"
  },
  "bugs": {
    "url": "https://github.com/rimiti/aclify/issues"
  },
  "engines": {
    "node": ">=6.9"
  },
  "engineStrict": true,
  "homepage": "https://github.com/rimiti/aclify#readme",
  "dependencies": {
    "async": "^2.6.0",
    "babel-runtime": "~6.26.0",
    "bluebird": "^3.5.1",
    "lodash": "^4.17.5",
    "mongodb": "^3.0.4",
    "mysql2": "^1.5.2",
    "redis": "^2.8.0",
    "sequelize": "^4.36.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "~6.26.0",
    "babel-eslint": "~8.2.2",
    "babel-jest": "~22.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "~6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "~6.26.0",
    "babel-plugin-transform-flow-strip-types": "~6.22.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-stage-3": "~6.24.1",
    "eslint": "~4.18.2",
    "eslint-config-airbnb-base": "~12.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "~2.9.0",
    "flow-bin": "~0.67.1",
    "jest": "~22.4.2",
    "node-mocks-http": "^1.5.8"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}
